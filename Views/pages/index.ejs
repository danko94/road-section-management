      <div class="page-wrapper mdc-toolbar-fixed-adjust">
        <main class="content-wrapper">
          <div class="mdc-layout-grid">
            <div class="mdc-layout-grid__inner">
           
              <% cards.forEach(function(card) { %>
              <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-3-desktop mdc-layout-grid__cell--span-4-tablet">
                <%- include ('../partials/Cards.ejs',{title:card['title'], number:card['num'], icon:card['icon']}) %>
              </div>
              <% }); %>
             
              <div class="mdc-layout-grid__cell stretch-card mdc-layout-grid__cell--span-12">
                <div class="mdc-card">
                  <div class="d-flex justify-content-between">
                    <h4 class="card-title mb-0">Cars by Section</h4>
                    
                  </div>
                  <div class="d-block d-sm-flex justify-content-between align-items-center">
                      <h5 class="card-sub-title mb-2 mb-sm-0">Number of cars on each section</h5>
                      <div class="menu-button-container">
                        
                        <div class="mdc-menu mdc-menu-surface" tabindex="-1">
                          <ul class="mdc-list" role="menu" aria-hidden="true" aria-orientation="vertical">
                            <li class="mdc-list-item" role="menuitem">
                              <h6 class="item-subject font-weight-normal">Back</h6>
                            </li>
                            <li class="mdc-list-item" role="menuitem">
                              <h6 class="item-subject font-weight-normal">Forward</h6>
                            </li>
                            <li class="mdc-list-item" role="menuitem">
                              <h6 class="item-subject font-weight-normal">Reload</h6>
                            </li>
                            <li class="mdc-list-divider"></li>
                            <li class="mdc-list-item" role="menuitem">
                              <h6 class="item-subject font-weight-normal">Save As..</h6>
                            </li>
                          </ul>
                        </div>
                      </div>
                  </div>
                  <div class="mdc-layout-grid__inner mt-2">
                    <div class="mdc-layout-grid__cell mdc-layout-grid__cell--span-6 mdc-layout-grid__cell--span-8-tablet">
                        <div class="table-responsive">
                          <table class="table dashboard-table">
                            <tbody>
                              <tr>
                                <td>
                                  <i class="fas fa-road"></i> Section 1</td>
                                <td><%= sections[0] %></td>
                                <td class=" font-weight-medium"> <%= Math.round(sections[0]*100/(total+0.0001)) %> % </td>
                              </tr>
                              <tr>
                                <td> <i class="fas fa-road"></i> Section 2</td>
                                <td><%= sections[1] %></td>
                                <td class=" font-weight-medium"> <%= Math.round(sections[1]*100/(total+0.0001)) %> % </td>
                              </tr>
                              <tr>
                                <td> <i class="fas fa-road"></i> Section 3</td>
                                <td><%= sections[2] %></td>
                                <td class=" font-weight-medium"> <%= Math.round(sections[2]*100/(total+0.0001)) %> % </td>
                              </tr>
                              <tr>
                                <td> <i class="fas fa-road"></i> Section 4</td>
                                <td><%= sections[3] %></td>
                                <td class=" font-weight-medium"> <%= Math.round(sections[3]*100/(total+0.0001)) %> % </td>
                              </tr>
                              <tr>
                                <td> <i class="fas fa-road"></i> Section 5</td>
                                <td><%= sections[4] %></td>
                                <td class=" font-weight-medium"> <%= Math.round(sections[4]*100/(total+0.0001)) %>  % </td>
                              </tr>
                            </tbody>
                          </table>
                        </div>
                    </div>
                    
                  </div>
                </div> 
